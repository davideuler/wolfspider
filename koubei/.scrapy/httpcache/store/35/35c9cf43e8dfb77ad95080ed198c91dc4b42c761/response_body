<script>
//crm
YUI().use('node-base', 'cookie', function(Y){
	var wraps = Y.all('.k2-crm-ad'),
		idSet = [];

	if(!wraps.isEmpty()){
		wraps.each(function(i){
			idSet.push(i.getAttribute('resourceid'));
		});

		Y.Get.script("http://marketing.taobao.com/home/delivery/AllContentByPageForKoubei.do?resourceIds=" + idSet.join(',') + "&userId=" + Y.Cookie.get('tk_ti') + "&t=" + new Date().getTime(), {
			onSuccess: function(){
				if(__content_results && (__content_results.length > 0)){
					var tObj = {};
					Y.Array.each(__content_results, function(i){
						tObj[i.id] = i;
					})
					wraps.each(function(wrap){
						wrap.one('a').setAttribute('href', tObj[wrap.getAttribute('resourceid')].link);
						wrap.one('img').setAttribute('src', tObj[wrap.getAttribute('resourceid')].content);
					});
				}
			}, 
			timeout: 10000,
			context: Y
		});
	}
});

YUI().use('node-base', function(Y){
    YUI.namespace('KB.channel', 'KB.cityid', 'KB.cityname', 'KB.time');
	if(Y.one('#k2-h-wrap')){
		var o = Y.one('#k2-h-wrap');
		YUI.KB.channel = o.getAttribute('data-channel') || '';
		YUI.KB.cityid = o.getAttribute('data-cityid') || 0;
		YUI.KB.cityname = o.getAttribute('data-cityname') || '';
                YUI.KB.time = new Date(parseInt(o.getAttribute('data-time')));
	}else{
		YUI.KB.channel = '';
		YUI.KB.cityid = 0;
		YUI.KB.cityname = '';
                YUI.KB.time = new Date();
	}

	var ipt = Y.one('#k2-h-search-ipt');
	function randomNumForSearch(n){
	  var result="";
	  for(var i=0;i<n;i++)
		result+=Math.floor(Math.random()*10);
	  return result;
	}
	
	if(Y.one('#k2-h-btn-map')){
		Y.one('#k2-h-btn-map').on('click', function(){
			var cache = randomNumForSearch(7);
			var cImg = document.createElement('img');
			cImg.src = 'http://www.atpanel.com/search?cache=' + cache + '&kind=' + 'kb.header.search.ditu';
			
			var keyword = ipt.get('value'),
				charset;
			if(('huangye' === YUI.KB.channel) || ('shanghu' === YUI.KB.channel)){
				charset = 'gbk';
			}else{	
				charset = 'utf8';
			}
			location.href = 'http://ditu.koubei.com/mapsearch/mapsearch.html?ei=' + charset + '&keyword=' + encodeURI(keyword) + '&city=' + YUI.KB.cityid + '#inview=0';
		});
	}
	if(Y.one('#k2-h-btn-youhui')){
		function goYouhui(){
			var keyword = ipt.get('value');
			location.href = 'http://www.koubei.com/fuwu/list.html?frontCategoryId=944&keyword=' + encodeURI(keyword) + '&city=' + YUI.KB.cityid;
		}
		Y.one('#k2-h-btn-youhui').on('click', function(){
			goYouhui();
		});
		Y.one('#k2-h-search-ipt').on('keyup', function(e){
			if(e.keyCode === 13){
				goYouhui();
			}
		});
	}
	
	if(Y.one('#k2-h-btn-order')){
		function goOrder(){
			var keyword = ipt.get('value');
			location.href = 'http://waimai.koubei.com/web/takeoutStoreList.html?orderBy=order_quantity&sort=DESC&keywordName=' + encodeURI(keyword) + '&city=' + YUI.KB.cityid;
		}
		Y.one('#k2-h-btn-order').on('click', function(){
			goOrder();
		});
		Y.one('#k2-h-search-ipt').on('keyup', function(e){
			if(e.keyCode === 13){
				goOrder();
			}
		});
	}
	
	if(Y.one('#k2-h-btn-youhui') || Y.one('#k2-h-btn-order')){
		Y.one('#k2-h-btn-bendi').on('click', function(){
			var keyword = ipt.get('value');
			location.href = 'http://bendi.koubei.com/search/searchstore.html?ei=utf8&keyword=' + encodeURI(keyword) + '&city=' + YUI.KB.cityid;
		});
	}
});

YUI().use("autocomplete-base","autocomplete-plugin","autocomplete-highlighters", "datasource-io","datasource-jsonschema","node-pluginhost","cookie", function (Y) {
  if((YUI.KB.channel !== 'order') && (YUI.KB.channel !== 'youhui')){
var ds = new Y.DataSource.IO({
		//source: './souces.html'
		source : "/search/suggest.html"
	}),
	listTemplate ='<div class="k2-fix-float"><span class="k2-h-ac-t">{sug}</span><span class="k2-h-ac-n">约{des}家店</span></div>';
	ds.plug({fn : Y.Plugin.DataSourceJSONSchema, cfg : {
			schema : {
				resultListLocator : "results",
				resultFields: [{key:"sug"},{key:"des"}]
			}
	}});	
	var acNode = Y.one('#k2-h-search-ipt');
        if(acNode!=null ){
	acNode.plug(Y.Plugin.AutoComplete, {
		maxResults: 10,
		resultListLocator: function(response){
			return response;
		},
		resultTextLocator:'sug',	
		resultFormatter	: function(query,results){
			return Y.Array.map(results, function (result) {
				var list = result.raw;
				return Y.Lang.sub(listTemplate, {
					sug : list.sug,
					des : list.des	
				});
			});
		},
                source: ds,
		requestTemplate: '?style=store&cate=1&encode=1&city='+YUI.KB.cityid+'&keyword={query}'
	}).ac.on('select',function(ev){
		ev.preventDefault();
			if(Y.one('#h-sf')){
				Y.one('#h-sf').set('value', '99');
			}
if(Y.one('#k2-h-position')){
			var li = ev.itemNode,
				ul = li.ancestor('ul');
			Y.one('#k2-h-position').set('value', ul.get('children').indexOf(li) + 1);
		}
		this.ac.hide();
		this.set('value',ev.result.text.replace(/<b class='yk-highlight'>(.+?)<\/b>/,'$1'));
		Y.one('#k2-h-search-form').submit();
	},acNode);
	}


	
	//sourcekey
	if(Y.one('#k2-h-sourcekey')){
		acNode.on('keyup', function(){
			Y.one('#k2-h-sourcekey').set('value', acNode.get('value'));
		});
        }

}
     
});
YUI().use('get','node-base','node-style','event-base','event-mouseenter', 'event-mouseleave', function(Y) {	

	function initMenu(){
		Y.all('.k2-h-trigger').each(function(n){
			n.addClass('k2-h-arrow');
		});
		function showMenu(e){
			e.target.ancestor('.k2-h-trigger-warpper').one('.k2-h-menu-warpper').setStyle('visibility', 'visible');
		}
		function hideMenu(e){
			e.target.ancestor('.k2-h-trigger-warpper').one('.k2-h-menu-warpper').setStyle('visibility', 'hidden');
		}
		Y.on('mouseenter', function(e){ showMenu(e); }, '.k2-h-trigger');
		Y.on('mouseleave', function(e){ hideMenu(e); }, '.k2-h-menu-warpper'); 
		Y.on('mouseleave', function(e){
			if(!e.relatedTarget.ancestor('.k2-h-menu-warpper')){
				hideMenu(e);
			}
		}, '.k2-h-trigger'); 
	}


	var success = function(){
	
		var data  =  ykHData; 

		if(data.login==1&&data.userInfo.name){
Y.one('#k2-h-guanzhu-link').setAttribute('href', data.userInfo.fansUrl);
			Y.one('#k2-h-user-name').setContent(data.userInfo.name);
			Y.one('#k2-h-message-number').setContent(data.userInfo.message+"");
			var messageNum = parseInt(ykHData.userInfo.message);
			if(messageNum > 0){
				Y.one('#k2-h-message-number').setStyle('color', '#f60');
				Y.one('#k2-h-trigger-message').addClass('k2-h-trigger');
				if(ykHData.userInfo.sixin>0){
					Y.one('#k2-h-sixin').setContent(ykHData.userInfo.sixin);
				}else{
					Y.one('#k2-h-sixin-wrap').setStyle('display', 'none');
				}
				if(ykHData.userInfo.tongzhi>0){
					Y.one('#k2-h-tongzhi').setContent(ykHData.userInfo.tongzhi);
				}else{
					Y.one('#k2-h-tongzhi-wrap').setStyle('display', 'none');
				}
				if(ykHData.userInfo.guanzhu>0){
					Y.one('#k2-h-guanzhu').setContent(ykHData.userInfo.guanzhu);
				}else{
					Y.one('#k2-h-guanzhu-wrap').setStyle('display', 'none');
				}
				if(ykHData.userInfo.huiying>0){
					Y.one('#k2-h-huiying').setContent(ykHData.userInfo.huiying);
				}else{
					Y.one('#k2-h-huiying-wrap').setStyle('display', 'none');
				}
				if(ykHData.userInfo.xianhua>0){
					Y.one('#k2-h-xianhua').setContent(ykHData.userInfo.xianhua);
				}else{
					Y.one('#k2-h-xianhua-wrap').setStyle('display', 'none');
				}
			}
			var tab = Y.one('#j-k2-h-tab'),
				panel = Y.one('#j-k2-h-menu');
			if(messageNum >= 0 && messageNum < 10){
				tab.setStyle('width', '57px');
				panel.setStyle('width', '60px');
			}else if(messageNum >= 10 && messageNum < 100){
				tab.setStyle('width', '63px');
				panel.setStyle('width', '63px');
			}else if(messageNum >= 100){
				tab.setStyle('width', '68px');
				panel.setStyle('width', '60px');
			}
			Y.all('.k2-h-login').setStyle('display','inline');
		}else{
			Y.all('.k2-h-not-login').setStyle('display','inline');
		}
		if (ykHData.ip == '121.0.29.244') {
			Y.one('#yk-jdm') && (Y.one('#yk-jdm').setStyle("display","block"));
		}
		initMenu();
	}

	var failure = function(){
		Y.all('.k2-h-not-login').setStyle('display','inline');
	}

    var tempDate = new Date();
    var cityid = YUI.KB.cityid, 
	offerId = 943;
	if(cityid == 2595){
		offerId = 957;
	}
	var sURL = "http://help.koubei.com/common/topNavigation.html?showUserInfo=1&advUserInfo=1&showInfoNum=1&showInfoNum=1&city="+cityid+"&offerId="+offerId+"&data="+tempDate.getTime();
    if(Y.one('#k2-h-user-name')){
		var transactionObj = Y.Get.script(sURL, {
			onSuccess: success,
			onFailure: failure,
			timeout: 20000,
			context: Y
		});
    }else{
		initMenu();
	}
});


	YUI().use('cookie','anim-base','node-base','event-base',function(Y){
                  if(Y.one('#k2-h-wrap')){
                var cityName = Y.one('#k2-h-wrap').getAttribute('data-cityname'),
                    channel = Y.one('#k2-h-wrap').getAttribute('data-channel');
		if(typeof(channel) != "undefined"&&channel!='hp'&&typeof(cityName) != "undefined"&&YUI.KB.cityid==2595&&Y.Cookie.get('MyKoubeiCity')!=2595&&Y.Cookie.get('MyKoubeiCity')!=null){
			var sHtml = "<div id='backcity-b' style='margin-top:-29px;position:fixed;_position:absolute;left:0;width:100%;height:3px;z-index:3000;background:url(http://k.kbcdn.com/product/common/tip/s.png) repeat-x 0 -58px;'></div><div id='backcity-tip' style='position:fixed;_position:absolute;left:35%;top:0;margin-top:-29px;z-index:3001;text-align:center;background:url(http://k.kbcdn.com/product/common/tip/s.png) repeat-x 0 -58px;'><div class='k2-inline-block' style='background:url(http://k.kbcdn.com/product/common/tip/s.png) no-repeat 0 0;width:185px;height:29px;'></div><div class='k2-inline-block' style='background:url(http://k.kbcdn.com/product/common/tip/s.png) repeat-x 0 -54px;height:29px;'><div class='k2-inline-block' style='width:128px;background:url(http://k.kbcdn.com/product/common/tip/s.png) -170px -29px;height:29px;padding-left:10px;'><div style='background:url(http://k.kbcdn.com/product/common/tip/s.png) right -29px;height:29px;padding-right:10px;line-height:28px;'><a id='backtip-link' href='http://www.koubei.com' target='_self' style='color:#000;'>回到"+cityName+"首页</a></div></div></div><div id='cls-tip' class='k2-inline-block' style='background:url(http://k.kbcdn.com/product/common/tip/s.png) no-repeat right 0;width:48px;height:29px;cursor:pointer;'></div></div><div id='backcity-tip-c' style='position:fixed;_position:absolute;left:45%;top:0;margin-top:-29px;z-index:3001;text-align:center;background:url(http://k.kbcdn.com/product/common/tip/s.png) repeat-x 0 -58px;height:3px;width:86px;'><div style='width:86px;margin:0 auto;height:29px;'><div id='ops-tip' style='background:url(http://k.kbcdn.com/product/common/tip/s.png) no-repeat 0 -29px;height:29px;cursor:pointer;'></div></div></div>";
			oTip = Y.Node.create(sHtml);
			Y.one('body').insertBefore(oTip,document.body.firstChild);

			
