		var oTip = document.createElement('div');
		oTip.innerHTML = sHtml;
		YK.D.insertBefore(oTip,document.body.firstChild);
		
		var myAnim = new YAHOO.util.Anim('backcity-tip', {
			marginTop: {
					to: 0 
			} 
		}, 0.6, YAHOO.util.Easing.easeIn);
		var myAnim2 = new YAHOO.util.Anim('backcity-tip-c', {
				marginTop: {
						to: 0 
				} 
		}, 0.3);

		var myAnim3 = new YAHOO.util.Anim('backcity-b', {
				marginTop: {
						to: 0 
				} 
		}, 0.6, YAHOO.util.Easing.easeIn);

		if(YAHOO.util.Cookie.get('backCityTip')=='1'){
			YK.D.get('backcity-tip-c').style.marginTop=0;
			YK.D.get('backcity-b').style.marginTop=0;
		}else{
			myAnim.animate();
			myAnim3.animate();
		}

		function randomNumForSearch(n){
			 var result="";
			 for(var i=0;i<n;i++)
				 result+=Math.floor(Math.random()*10);
			 return result;
	 }
	 var cImg = document.createElement('img');
	 cImg.src = 'http://www.atpanel.com/search?cache=' + randomNumForSearch(7) + '&kind=' + 'kb.city.head.show';
		
	 YK.on('backtip-link','mousedown',function(ev){
		 var cache = randomNumForSearch(7);
		 var cImg = document.createElement('img');
		 cImg.src = 'http://www.atpanel.com/search?cache=' + cache + '&kind=' + 'kb.city.head.click';
