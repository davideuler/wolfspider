(function(){
	function randomNumForSearch(n){
		var result="";
    	for(var i=0;i<n;i++)
        	result+=Math.floor(Math.random()*10);
    	return result;		
	}

	function monitorForSearch(t){
		var cache = randomNumForSearch(7);
		var cImg = document.createElement('img');
		cImg.src = "http://www.atpanel.com/search?cache=" + cache + "&kind=" + t;
		return true;
	}
	if(YK.vars&&YK.vars.channel){
		switch(YK.vars.channel){
			case "hp" :
				t = "kb.temp.searchbt.hp";
				break;
			case "youhui" :	
				t = "kb.temp.searchbt.youhui";
				break;
			case "pin" :	
				t = "kb.temp.searchbt.huodong";
				break;
			case "huangye" :	
				t = "kb.temp.searchbt.dian";
				break;
			case "fenlei" :	
				t = "kb.temp.searchbt.fuwu";
				break;		
			case "newbbs" :	
				t = "kb.temp.searchbt.bbs";
				break;	
			case "ka" :	
				t = "kb.temp.searchbt.kbcard";
				break;
			default : 
				t = "kb.temp.searchbt.other";
		}
		YK.on('yk-h-search-form','submit',function(){
			monitorForSearch(t);
			return true;
		});
	}
})();


function initMenu(){
	var _E = YK.E,_D = YK.D,_query = YAHOO.util.Selector.query;
	function showMenu(e){
		_D.setStyle(_D.getElementsByClassName('k2-h-menu-warpper','div',_D.getAncestorByClassName(_E.getTarget(e),'k2-h-trigger-warpper')),'visibility','visible');
	}
	function hiddeMenu(e){
		_D.setStyle(_D.getElementsByClassName('k2-h-menu-warpper','div',_D.getAncestorByClassName(_E.getTarget(e),'k2-h-trigger-warpper')),'visibility','hidden');
	}
	function targetLeave(e){
		var target = _E.getRelatedTarget(e);
	    if(target!= this && !_D.getAncestorByClassName(target,'k2-h-trigger-warpper') || _D.getAncestorByClassName(target,'k2-h-trigger-warpper') != _D.getAncestorByClassName(_E.getTarget(e),'k2-h-trigger-warpper')){
	    		hiddeMenu(e);
	    }
	}
	_D.addClass(_D.getElementsByClassName('k2-h-trigger'),'k2-h-arrow');
	_E.on(_query('.k2-h-trigger'),'mouseover',function(e){ showMenu(e); });
	_E.on(_query('.k2-h-trigger'),'mouseout',function(e){ targetLeave(e); });
	_E.on(_query('.k2-h-menu-warpper'),'mouseout',function(e){ targetLeave(e); });
}

//lazy load user info
var tempDate = new Date();
var getUrl ='http://help.koubei.com/common/topNavigation.html?showUserInfo=1&advUserInfo=1&showInfoNum=1&showInfoNum=1&date='+tempDate.getTime();

if (YK.D.get('k2-h-user-name')) {
	YAHOO.util.Get.script(getUrl, {
		onSuccess: function() {
	
			if (typeof ykHData == 'undefined') return;
			if (ykHData.login == 0) {
				YK.D.setStyle(YK.D.getElementsByClassName('k2-h-not-login'), 'display', 'block');
			} else {
                                YK.D.get('k2-h-guanzhu-link') && (YK.D.get('k2-h-guanzhu-link').href = ykHData.userInfo.fansUrl);
				YK.D.get('k2-h-user-name') && (YK.D.get('k2-h-user-name').innerHTML = ykHData.userInfo.name);
				YK.D.get('k2-h-message-number') && (YK.D.get('k2-h-message-number').innerHTML = ykHData.userInfo.message);
				var messageNum = parseInt(ykHData.userInfo.message);
				if(messageNum > 0){
					YK.D.get('k2-h-message-number') && (YK.D.setStyle(YK.D.get('k2-h-message-number'), 'color', '#f60'));
					YK.D.addClass(YK.D.get('k2-h-trigger-message'), 'k2-h-trigger');
					if(ykHData.userInfo.sixin > 0 && YK.D.get('k2-h-sixin')){
						YK.D.get('k2-h-sixin').innerHTML = ykHData.userInfo.sixin;
					}else{
						YK.D.setStyle(YK.D.get('k2-h-sixin-wrap'), 'display', 'none');
					}
					if(ykHData.userInfo.tongzhi > 0 && YK.D.get('k2-h-tongzhi')){
						YK.D.get('k2-h-tongzhi').innerHTML = ykHData.userInfo.tongzhi;
					}else{
						YK.D.setStyle(YK.D.get('k2-h-tongzhi-wrap'), 'display', 'none');
					}
					if(ykHData.userInfo.guanzhu > 0 && YK.D.get('k2-h-guanzhu')){
						YK.D.get('k2-h-guanzhu').innerHTML = ykHData.userInfo.guanzhu;
					}else{
						YK.D.setStyle(YK.D.get('k2-h-guanzhu-wrap'), 'display', 'none');
					}
					if(ykHData.userInfo.huiying > 0 && YK.D.get('k2-h-huiying')){
						YK.D.get('k2-h-huiying').innerHTML = ykHData.userInfo.huiying;
					}else{
						YK.D.setStyle(YK.D.get('k2-h-huiying-wrap'), 'display', 'none');
					}
					if(ykHData.userInfo.xianhua > 0 && YK.D.get('k2-h-xianhua')){
						YK.D.get('k2-h-xianhua').innerHTML = ykHData.userInfo.xianhua;
					}else{
						YK.D.setStyle(YK.D.get('k2-h-xianhua-wrap'), 'display', 'none');
					}
				}
				var tab = YK.D.get('j-k2-h-tab'),
					panel = YK.D.get('j-k2-h-menu');
				if(messageNum >= 0 && messageNum < 10){
					YK.D.setStyle(tab, 'width', '57px');
					YK.D.setStyle(panel, 'width', '60px');
				}else if(messageNum >= 10 && messageNum < 100){
					YK.D.setStyle(tab, 'width', '63px');
					YK.D.setStyle(panel, 'width', '63px');
				}else if(messageNum >= 100){
					YK.D.setStyle(tab, 'width', '68px');
					YK.D.setStyle(panel, 'width', '60px');
				}
				YK.D.setStyle(YK.D.getElementsByClassName('k2-h-login'), 'display', 'block');
			}
			if (ykHData.ip == '121.0.29.244') {
				YK.D.get('yk-jdm') && (YK.D.get('yk-jdm').style.display = 'block');
			}
			initMenu();
                       (function(){
				var list = YAHOO.util.Selector.query('.kb-btn-list li');
				if(ykHData.login && list.length==4){
					list[0].innerHTML = '<a href="http://my.koubei.com/my/myhome.html" rel="nofollow" target="_blank">我的口碑</a>';
					list[1].innerHTML = '<a href="http://e.koubei.com/jiameng" rel="nofollow" target="_blank">商户加盟</a>';
				}
			})();

		}
	});
}else{
	initMenu();
}


