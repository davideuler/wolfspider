					node:'#backcity-tip',
				 	to:	{
						marginTop: 0 
					},
					duration:0.6
			});
			var myAnim2 = new Y.Anim({
					node:'#backcity-tip-c',
				 	to:	{
						marginTop: 0 
					},
					duration:0.3
			});
			var myAnim3 = new Y.Anim({
					node:'#backcity-b',
				 	to:	{
