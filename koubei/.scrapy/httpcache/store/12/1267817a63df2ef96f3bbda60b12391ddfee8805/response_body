			myAnim.attributes.marginTop={to:-29};
			myAnim.animate();
			myAnim2.onComplete.unsubscribe();
			myAnim.onComplete.subscribe(function(){
				myAnim2.attributes.marginTop={to:0}
				myAnim2.animate();
