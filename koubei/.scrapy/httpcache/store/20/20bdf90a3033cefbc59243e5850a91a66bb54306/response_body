(function(){
  var crmArray = YK.D.getElementsByClassName('yk-crm-ad');
  var crmParm = [];
  //{33:div,38:div}
  YAHOO_config.crmCallBack = {};
  for(var i = 0,j = crmArray.length;i < j ; i++){
    crmParm[i] = crmArray[i].getAttribute('resourceid');
    YAHOO_config.crmCallBack[crmParm[i]] = crmArray[i];
  }

  if(crmParm.length > 0){
    YAHOO.util.Get.script('http://marketing.taobao.com/home/delivery/AllContentByPageForKoubei.do?resourceIds=' + crmParm.join(',')+"&userId="+YAHOO.util.Cookie.get("tk_ti"),{
      onSuccess: function(){
        
        for(var j = __content_results.length;j>0;j--){
          var tempObj = __content_results[j-1];
          var tempDom = YAHOO_config.crmCallBack[tempObj['id']];
          if(tempDom.getAttribute('defer') == 1){
            continue;
          }
          tempDom.innerHTML = '<a target="_blank" href="' + tempObj.link + '"><img width="' + tempDom.getAttribute('data-width') + '" height="' + tempDom.getAttribute('data-height') + '" src="' + tempObj.content + '"/></a>';
          delete YAHOO_config.crmCallBack[tempObj['id']];
        }
      },
      onFailure: function(){
        
      },
      charset:'GBK'
    });
  }
})();

//顶通
(function(){

	var D = YK.D,
		E = YK.E;

	//可伸缩
	if(D.inDocument('k2-dt-extendable')){

		var warp = D.get('k2-dt-extendable'),
			expanded = D.get('k2-dt-expanded'),
			collapsed = D.get('k2-dt-collapsed'),
			close = D.get('k2-dt-close'),
			open = D.get('k2-dt-open');
		
		var cookieType = warp.getAttribute('data-cookie-type'),
			cookieName = warp.getAttribute('data-cookie-name'),
			cookieDay = warp.getAttribute('data-cookie-day'),
			cookieDomain = warp.getAttribute('data-cookie-domain');

		var expandedHeight = warp.getAttribute('data-expanded-height'),
			collapsedHeight = warp.getAttribute('data-collapsed-height');
		
		var duration = 0.35;

		var cookieDate = cookieDay.split(' ')[0],
			coolieHours = cookieDay.split(' ')[1];
			
		var year = parseInt(cookieDate.split('-')[0]),
			month = parseInt(cookieDate.split('-')[1]),
			day = parseInt(cookieDate.split('-')[2]),
			hour = parseInt(coolieHours);
			
		var dExpires = new Date(year, month - 1, day, hour, 0, 0, 0);
		
		E.on(close, 'click', function(e){
			E.stopEvent(e);

			var a = new YAHOO.util.Anim(expanded, {
				height: {to: collapsedHeight}
			}, duration, YAHOO.util.Easing.easeOut);
			a.onComplete.subscribe(function(){
				D.setStyle(collapsed, 'display', 'block');
				D.setStyle(expanded, 'display', 'none');
			});
			a.animate();
			
			YAHOO.util.Cookie.set(cookieName, 'hide', {
				path: '/',
				domain: cookieDomain,
				expires: dExpires
			});
		});
		
		E.on(open, 'click', function(e){
			E.stopEvent(e);

			D.setStyle(expanded, 'height', collapsedHeight + 'px');
			D.setStyle(expanded, 'display', 'block');
			D.setStyle(collapsed, 'display', 'none');
	
			var a = new YAHOO.util.Anim(expanded, {
				height: {to: expandedHeight}
			}, duration, YAHOO.util.Easing.easeOut);
			a.animate();
			
			YAHOO.util.Cookie.set(cookieName, 'show', {
				path: '/',
				domain: cookieDomain,
				expires: dExpires
			});
		});
		
		E.on(close, 'mouseover', function(){D.setStyle(close, 'backgroundPosition', '0 100%');});
		E.on(close, 'mouseout', function(){D.setStyle(close, 'backgroundPosition', '0 0');});
		E.on(open, 'mouseover', function(){D.setStyle(open, 'backgroundPosition', '100% 100%');});
		E.on(open, 'mouseout', function(){D.setStyle(open, 'backgroundPosition', '100% 0');}); 
	}

	//可关闭
	if(D.inDocument('k2-dt-closeable')){

		var warp = D.get('k2-dt-closeable'),
			hide = D.get('k2-dt-hide');
		
		var cookieType = warp.getAttribute('data-cookie-type'),
			cookieName = warp.getAttribute('data-cookie-name'),
			cookieDay = warp.getAttribute('data-cookie-day'),
			cookieDomain = warp.getAttribute('data-cookie-domain');

		var cookieDate = cookieDay.split(' ')[0],
			coolieHours = cookieDay.split(' ')[1];
			
		var year = parseInt(cookieDate.split('-')[0]),
			month = parseInt(cookieDate.split('-')[1]),
			day = parseInt(cookieDate.split('-')[2]),
			hour = parseInt(coolieHours);
			
		var dExpires = new Date(year, month - 1, day, hour, 0, 0, 0);
		
		E.on(hide, 'click', function(){
			D.setStyle(warp, 'display', 'none');
			YAHOO.util.Cookie.set(cookieName, 'hide', {
				path: '/',
				domain: cookieDomain,
				expires: dExpires
			});
		});
		
		E.on(hide, 'mouseover', function(){D.setStyle(hide, 'backgroundPosition', '0 100%');});
		E.on(hide, 'mouseout', function(){D.setStyle(hide, 'backgroundPosition', '0 0');});
	}

})();

//鲜辣点评埋点
(function(){
	var url = location.href,
		cookieName = 'reviewtuijian',
		cookieValue = '1001275',
		cookieDomain = '.koubei.com';
	if(url.indexOf('/tuijian') > 0){
		YAHOO.util.Cookie.set(cookieName, cookieValue, {
			path: '/',
			domain: cookieDomain
		});
	}else if((url.indexOf('/tuijian') === -1) && (url.indexOf('/review') === -1)){
		YAHOO.util.Cookie.remove(cookieName, {
			path: '/',
			domain: cookieDomain
		});
	}
})();
</script>





